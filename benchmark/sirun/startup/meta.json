{
  "name": "startup",
  "setup": "node setup",
  "run": "node startup-test.js",
  "cachegrind": true,
  "iterations": 10,
  "variants": {
    "control": {
      "env": {
        "USE_TRACER": "0"
      }
    },
    "with-internal-tracer": {
      "baseline": "control",
      "env": {
        "USE_INTERNAL_TRACER": "1",
        "DD_TRACE_AGENT_PROTOCOL_VERSION": "0.5"
      }
    },
    "with-internal-tracer-bundle": {
      "baseline": "control",
      "env": {
        "USE_INTERNAL_TRACER_BUNDLE": "1",
        "DD_TRACE_AGENT_PROTOCOL_VERSION": "0.5"
      }
    },
    "with-tracer": {
      "baseline": "control",
      "env": {
        "USE_TRACER": "1"
      }
    },
    "control-everything": {
      "env": {
        "USE_TRACER": "0",
        "EVERYTHING": "1"
      }
    },
    "with-tracer-everything": {
      "baseline": "control-everything",
      "env": {
        "USE_TRACER": "1",
        "EVERYTHING": "1"
      }
    }
  }
}
